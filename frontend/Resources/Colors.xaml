<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colors -->
    <Color x:Key="PrimaryColorColor">#333333</Color>
    <!--<Color x:Key="PrimaryColorColor">#F2001C</Color>-->
    <SolidColorBrush x:Key="PrimaryColor" Color="{StaticResource PrimaryColorColor}" />
    
    <Color x:Key="PrimaryFocusColorColor">#555555</Color>
    <!--<Color x:Key="PrimaryFocusColorColor">#B30015</Color>-->
    <SolidColorBrush x:Key="PrimaryFocusColor" Color="{StaticResource PrimaryFocusColorColor}" />

    <Color x:Key="SecondaryColorColor" >White</Color>
    <SolidColorBrush x:Key="SecondaryColor" Color="{StaticResource SecondaryColorColor}" />

    <Color x:Key="SecondaryFocusColorColor" >#dddddd</Color>
    <SolidColorBrush x:Key="SecondaryFocusColor" Color="{StaticResource SecondaryFocusColorColor}" />
    
</ResourceDictionary>